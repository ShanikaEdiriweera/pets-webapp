{
  "name": "pets-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "fuse.js": "^6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "testCRA": "react-scripts test",
    "test": "jest",
    "eject": "react-scripts eject",
    "jest": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "^26.6.0",
    "cypress": "^10.9.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "jest": "^26.6.0",
    "moxios": "^0.4.0",
    "msw": "^0.47.3",
    "nock": "^13.2.9",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/__tests__/dotenvConfig.js"
    ],
    "testEnvironment": "node"
  }
}
